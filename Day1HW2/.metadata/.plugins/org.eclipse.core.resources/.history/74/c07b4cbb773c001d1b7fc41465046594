package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {
		int number = 1;
//		int remainder = number % 2;
		if (number < 2) {
			System.out.println("Sayı Asal Değildir.");
		} else {
			for (int i = 1; i < number; i++) {
				if (number % i == 0) {
					System.out.println("Sayı Asal Değildir.");
					break;
				} else if (i == number - 1) {
					System.out.println("Sayı Asaldır.");
				}
			}
		}

	}

}
